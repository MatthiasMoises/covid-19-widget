{
  "name": "covid-19-widget",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "sync-s3-production": "aws s3 sync --acl public-read --delete dist/ s3://covid19-widget-production/widget",
    "sync-s3-staging": "aws s3 sync ./dist s3://covid19-widget-staging",
    "copy-appjs-production": "aws s3 cp s3://covid19-widget-production/widget/js/app.js s3://covid19-widget-production/widget/app.js",
    "copy-appjs-staging": "aws s3 cp s3://covid19-widget-staging/js/app.js s3://covid19-widget-staging/app.js",
    "invalidate-production-distribution": "aws cloudfront create-invalidation --distribution-id E2P9PVRM8ZAMD4 --paths \"/*\"",
    "invalidate-staging-distribution": "aws cloudfront create-invalidation --distribution-id E2K6GX0SGUZOVD --paths \"/*\"",
    "deploy-production": "npm run build && npm run sync-s3-production && npm run copy-appjs-production && npm run invalidate-production-distribution",
    "deploy-staging": "npm run build -- --mode staging && npm run sync-s3-staging && npm run copy-appjs-staging && npm run invalidate-staging-distribution",
    "lint": "vue-cli-service lint",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "postcss-prefix-selector": "^1.7.2",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.16.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-pwa": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.1.2",
    "material-design-icons-iconfont": "^5.0.1",
    "node-sass": "^4.12.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
